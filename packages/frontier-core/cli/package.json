{
  "name": "@fronter/cli",
  "version": "32.0.0",
  "description": "REPLACE_DESCRIPTION",
  "license": "UNLICENSED",
  "author": "REPLACE_AUTHOR|Realdecoy Labs",
  "main": "dist/index.js",
  "module": "module/index.js",
  "files": [
    "dist/",
    "module/",
    "types/",
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "bin": {
    "frontier": "./bin/run"
  },
  "homepage": "https://github.com/realdecoy/frontier",
  "scripts": {
    "build": "build-package",
    "build:watch": "build-package --watch",
    "postpack": "rimraf oclif.manifest.json",
    "clean": "rimraf dist esnext module types",
    "fix": "run-s lint:es:fix lint:md:fix",
    "lint": "run-s lint:es",
    "lint:es": "cross-env LOCAL_TS=1 eslint --ignore-path ../../../.eslintignore \"**/*\"",
    "lint:es:ci": "yarn lint:es --format",
    "lint:es:fix": "yarn lint:es --fix",
    "rebuild": "run-s clean build",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "test:coverage": "run-s test:ci test:coverage:open",
    "test:coverage:open": "open ./test-results/index.html",
    "test:watch": "jest --watch",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "posttest": "yarn lint",
    "typecheck": "tsc --noEmit",
    "version": "oclif readme && git add README.md"
  },
  "sideEffects": false,
  "types": "types/index.d.ts",
  "dependencies": {
    "@babel/runtime": "^7.15.0",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1"
  },
  "devDependencies": {
    "@frontier/lib-builder": "^1.0.0",
    "@oclif/test": "^2",
    "@types/node": "^16.9.4",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "oclif": "^2",
    "ts-node": "^10.2.1"
  },
  "repository": "realdecoy/frontier",
  "bugs": "https://github.com/realdecoy/frontier/issues",
  "browserslist": [
    "chrome 38",
    "ios >= 9"
  ],
  "oclif": {
    "bin": "frontier",
    "dirname": "frontier",
    "commands": "./module/commands",
    "scope": "frontier",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
